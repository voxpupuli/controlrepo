{
  "name": "voxpupuli-profiles",
  "version": "0.1.0",
  "summary": "a list of profiles for our cloud instances",
  "author": "Vox Pupuli",
  "source": "https://github.com/voxpupuli/voxpupu.li.git",
  "project_page": "https://github.com/voxpupuli/voxpupu.li",
  "issues_url": "https://github.com/voxpupuli/voxpupu.li/issues",
  "license": "AGPL-3.0",
  "tags": [
    "control repo",
    "controlrepo",
    "profiles",
    "vox pupuli",
    "voxpupuli"
  ],
