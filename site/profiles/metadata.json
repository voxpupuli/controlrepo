    {
      "name": "puppet/r10k",
      "version_requirement": ">= 10.1.1 < 11.0.0"
    },
    {
      "name": "puppet/grafana",
      "version_requirement": ">= 10.0.1 < 11.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 8.3.0 < 9.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 8.1.0 < 9.0.0"
    }
  ]
